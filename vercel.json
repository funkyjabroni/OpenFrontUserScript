{
  "version": 2,
  "buildCommand": "npm run build-prod",
  "outputDirectory": "static",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/favicon.ico",
      "destination": "/resources/images/Favicon.svg"
    },
    {
      "source": "/manifest.json",
      "destination": "/resources/manifest.json"
    },
    {
      "source": "/test-api/:path*",
      "destination": "https://34.36.164.25/api/:path*"
    },
    {
      "source": "/test-api-80/:path*",
      "destination": "https://34.36.164.25/api/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://34.36.164.25/api/:path*"
    },
    {
      "source": "/cosmetics.json",
      "destination": "https://34.36.164.25/cosmetics.json"
    },
    {
      "source": "/w0/:path*",
      "destination": "https://34.36.164.25/:path*"
    },
    {
      "source": "/w1/:path*",
      "destination": "https://34.36.164.25/:path*"
    },
    {
      "source": "/w2/:path*",
      "destination": "https://34.36.164.25/:path*"
    },
    {
      "source": "/((?!js|images|fonts|binary|text|resources|api|w[0-2]|test-api|test-api-80).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/binary/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/text/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/resources/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "GAME_ENV": "prod"
  },
  "cleanUrls": true,
  "trailingSlash": false
}

